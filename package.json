  "private": true,
  "scripts": {
    "start": "react-native start",
    "start-reset-cache": "rm -fr $TMPDIR/react-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all && react-native start -- --reset-cache",
    "postinstall": "node ./scripts/stfu.js && npx jetifier",
    "pod-install": "cd ios && pod install && cd ..",
    "fresh-install": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && yarn fresh-pods",
    "fresh-pods": "cd ios && rm -rf Podfile.lock && rm -rf Pods && pod install && cd ..",
    "test": "jest",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build-android:clean": "bash scripts/build-android.sh clean",
    "build-android:debug": "bash scripts/build-android.sh debug",
    "build-android:staging": "bash scripts/build-android.sh staging",
    "build-android:release": "bash scripts/build-android.sh release",
    "codepush": "bash scripts/codepush.sh",
    "ios:update-certificates": "cd ios && bundle exec fastlane bootstrap",
    "ios:get-certificates": "cd ios && READONLY=true bundle exec fastlane bootstrap"
  },
